{% extends 'base.html.twig' %}

{% block title 'Gestion des proprietes' %}

{% block body %}
<div class="container mt-4">

<h1>Gestion des proprietes</h1>

<table class="table table-striped">
<thead>
<tr>
    <th> Titre du bien</th>
        <th> Actions</th>
</tr>
</thead>
<tbody>
 {% for property in properties %}

<tr>
